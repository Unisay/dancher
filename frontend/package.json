{
  "name": "dancher-frontend",
  "version": "0.0.1",
  "description": "Frontend for the Dancher",
  "main": "support/index.js",
  "keywords": [
    "dancher"
  ],
  "scripts": {
    "postinstall": "bower cache clean && bower install",
    "clean": "rimraf static/dist",
    "build": "npm run clean && webpack --config ./webpack.config.js --progress --profile --colors",
    "watch": "npm run clean && webpack-dev-server --content-base static/ --content-base bower_components/material-design-lite/ --hot --inline --config webpack.config.js",
    "serve": "npm run build && serve static",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/unisay/dancher.git"
  },
  "author": "Yuriy V. Lazarev",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/unisay/dancher/issues"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "dependencies": {
    "babili-webpack-plugin": "^0.1.2",
    "bower": "^1.7.9",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "purescript": "^0.11.4",
    "purescript-psa": "^0.5.1",
    "purs-loader": "^3.0.0",
    "rimraf": "^2.5.2",
    "serve": "^5.1.5",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.5.4",
    "xhr2": "^0.1.3"
  }
}
