{
  "name": "dancher-frontend",
  "version": "0.0.1",
  "description": "Frontend for the Dancher",
  "keywords": [
    "dancher"
  ],
  "scripts": {
    "postinstall": "bower cache clean && bower install",
    "clean": "rm -f static/bundle.js",
    "build": "npm run clean && webpack --config ./webpack.config.js --progress --profile --colors",
    "watch": "npm run clean && webpack-dev-server --content-base static/ --hot --inline --config webpack.config.js",
    "serve": "npm run build && serve static",
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/unisay/dancher.git"
  },
  "author": "Yuriy V. Lazarev",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/unisay/dancher/issues"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "aliasify": {
    "aliases": {
      "react": "preact-compat",
      "react-dom": "preact-compat",
      "create-react-class": "preact-compat/lib/create-react-class"
    }
  },
  "dependencies": {
    "babili-webpack-plugin": "^0.1.2",
    "bower": "^1.7.9",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "purescript": "^0.11.6",
    "purescript-psa": "^0.5.1",
    "purs-loader": "^3.1.0",
    "serve": "^5.2.4",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.5.4",
    "xhr2": "^0.1.3"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babelify": "^7.3.0"
  }
}
